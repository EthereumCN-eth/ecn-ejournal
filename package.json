    "eslint-config-sznm": "^1.0.2",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "next-sitemap": "^3.1.43",
    "prettier": "^2.8.1",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.4"
